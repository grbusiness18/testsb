{
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "brokerCredentials": {
      "type": "object",
      "minProperties": 1,
      "additionalProperties": false,
      "patternProperties": {
        "^.+$": {
          "type": "string",
          "pattern": "^.+$"
        }
      }
    },
    "brokerCredentialsHash": {
      "type": "object",
      "minProperties": 1,
      "additionalProperties": false,
      "patternProperties": {
        "^.+$": {
          "type": "string",
          "pattern": "^sha256:[a-zA-Z0-9+/=]+:[a-zA-Z0-9+/=]+$"
        }
      }
    },
    "catalog": {
      "type": [
        "string",
        "object"
      ],
      "minLength": 1,
      "minProperties": 1
    },
    "serviceConfig": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "extend_credentials": {
            "$ref": "#/definitions/extendObject"
          },
          "extend_xssecurity": {
            "$ref": "#/definitions/extendObject"
          },
          "extend_catalog": {
            "type": "object"
          }
        }
      }
    },
    "hooks": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "onProvision": {
          "typeof": "function"
        },
        "onDeprovision": {
          "typeof": "function"
        },
        "onUpdate": {
          "typeof": "function"
        },
        "onLastOperation": {
          "typeof": "function"
        },
        "onBind": {
          "typeof": "function"
        },
        "onUnbind": {
          "typeof": "function"
        }
      }
    },
    "autoCredentials": {
      "type": "boolean"
    },
    "enableAuditLog": {
      "type": "boolean"
    },
    "credentialsProviderService": {
      "type": "string",
      "minLength": 1
    },
    "sbssRestrictedUserService": {
      "type": "string",
      "minLength": 1
    },
    "catalogSuffix": {
      "type": "string",
      "minLength": 1
    },
    "secureOutgoingConnections": {
      "type": "boolean"
    }
  },
  "definitions": {
    "extendObject": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "shared": {
          "type": "object"
        },
        "per_plan": {
          "type": "object",
          "additionalProperties": {
            "type": "object"
          }
        }
      }
    }
  }
}